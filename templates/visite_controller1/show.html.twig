{% extends 'base.html.twig' %}

{% block title %}Visite{% endblock %}

{% block body %}
    <h1>Visite</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ visite.id }}</td>
            </tr>
            <tr>
                <th>DatePrevue</th>
                <td>{{ visite.datePrevue ? visite.datePrevue|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateReelle</th>
                <td>{{ visite.dateReelle ? visite.dateReelle|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Duree</th>
                <td>{{ visite.duree }}</td>
            </tr>
            <tr>
                <th>CompteRenduInfirmiere</th>
                <td>{{ visite.compteRenduInfirmiere }}</td>
            </tr>
            <tr>
                <th>CompteRenduPatient</th>
                <td>{{ visite.compteRenduPatient }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_visite_controller1_index') }}">back to list</a>

    <a href="{{ path('app_visite_controller1_edit', {'id': visite.id}) }}">edit</a>

    {{ include('visite_controller1/_delete_form.html.twig') }}
{% endblock %}
